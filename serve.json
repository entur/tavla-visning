{
    "rewrites": [
        {
            "source": "**",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "**",
            "headers": [
                {
                    "key": "Content-Security-Policy",
                    "value": "frame-ancestors 'self' https://tavla.entur.no https://*.tavla.entur.no localhost:* http://localhost:*"
                }
            ]
        }
    ]
}