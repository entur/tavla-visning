    "*.{js,ts,tsx,css}": [
      "pnpm exec biome lint --fix ./src",
      "pnpm exec biome format --write ./src"
    ]
  },
  "dependencies": {
    "@entur/button": "3.4.1",
    "@entur/icons": "8.2.0",
    "@entur/loader": "0.5.35",
    "@entur/tokens": "3.20.0",
    "@entur/typography": "2.0.3",
    "@graphql-typed-document-node/core": "3.2.0",
    "abortcontroller-polyfill": "1.7.8",
    "clsx": "2.1.1",
    "lodash": "4.17.21",
    "nanoid": "5.1.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "swr": "2.3.6",
    "whatwg-fetch": "3.6.20"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.3",
    "@types/lodash": "4.17.20",
    "@types/node": "24.6.0",
    "@types/react": "19.1.16",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-legacy": "6.1.1",
    "@vitejs/plugin-react-swc": "4.1.0",
    "autoprefixer": "10.4.21",
    "core-js": "3.46.0",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "postcss": "8.5.6",
    "regenerator-runtime": "0.14.1",
    "tailwindcss": "3.4.14",
    "typescript": "5.9.3",
    "vite": "7.1.7"
  }
