{
  "name": "tavla-visning",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "browserslist": [
    "chrome > 47"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "pnpm exec biome lint ./src",
    "format": "pnpm exec biome format --write ./src",
    "preview": "vite preview",
    "prepare": "husky"
  },
